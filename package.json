{
  "name": "@wonderlandengine/spatial-audio",
  "version": "1.0.0",
  "description": "Spatial audio component for Wonderland Engine",
  "author": "Wonderland GmbH",
  "license": "MIT",
  "wonderlandengine": {},
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.js",
    "hrtf/hrtf_128.bin",
    "scripts/postinstall-script.js"
  ],
  "scripts": {
